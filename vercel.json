{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/build",
        "buildCommand": "cd client && npm run vercel-build"
      }
    },
    {
      "src": "api/hello.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/login.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/hello",
      "dest": "/api/hello.js"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/client/build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/client/build/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/client/build/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/client/build/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "/client/build/logo512.png"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ]
}